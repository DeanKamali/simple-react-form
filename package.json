{
  "name": "simple-react-form",
  "version": "1.7.3",
  "license": "MIT",
  "author": "Nicolás López <nicolaslopezj@me.com>",
  "description": "A library to make reusable form components in React and React Native",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolaslopezj/simple-react-form/tree/master/form"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "test": "standard \"src/**/*.js\" && jest",
    "watch": "watch npm\\ run\\ build ./src",
    "test-watch": "jest --watch",
    "lint": "grunt tslint"
  },
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android"
  ],
  "dependencies": {
    "dot-object": "^1.5.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "budo": "^8.0.3",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^15.1.1",
    "node-sass": "^3.4.2",
    "react": ">=15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": ">=15.0.0",
    "run-browser-babel": "^4.0.1",
    "standard": "*",
    "tape": "^4.0.0",
    "uglify-js": "^2.6.1"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib"
    ]
  },
  "standard": {
    "globals": [
      "it",
      "xit",
      "expect",
      "test",
      "jest",
      "MongoObject"
    ]
  }
}
